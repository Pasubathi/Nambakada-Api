{
  "name": "formdata-json",
  "version": "1.0.0",
  "description": "Converts FormData entries to JSON",
  "main": "index.js",
  "scripts": {
    "bundle": "browserify index.js -s formDataJSON -o build/index.js -t [ babelify --presets [ \"@babel/preset-env\" ] ]",
    "uglify": "uglifyjs build/index.js -cm > build/index.min.js",
    "build": "npm run bundle && npm run uglify",
    "test": "mocha"
  },
  "keywords": [
    "FormData",
    "form",
    "data",
    "JSON",
    "object",
    "convert",
    "transform",
    "browser",
    "tests"
  ],
  "author": "James Jeffery <jamesjefferyuk@protonmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "babelify": "^9.0.0",
    "browserify": "^16.2.2",
    "mocha": "^5.2.0",
    "uglify-js": "^3.4.7"
  }
}
